<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
    <Dialog id="OnCreate" title="Select Tab" class="sapUiResponsivePadding">
        <content>
            <VBox>
                <Text text="Please select one tab:" class="sapUiSmallMargin"/>
                <List 
                    id="tabList"
                    mode="SingleSelectMaster"
                    selectionChange="onListSelectionChange"
                >
                    <StandardListItem title="Project" type="Active" />
                    <StandardListItem title="Project Members" type="Active"/>
                    <StandardListItem title="Task" type="Active"/>
                    <StandardListItem title="Project Budget" type="Active"/>
                </List>
            </VBox>
        </content>
        <!-- <beginButton>
            <Button text="Proceed" press="onDialogProceed" />
        </beginButton> -->
        <endButton>
            <Button text="Cancel" press="onDialogCancel" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
